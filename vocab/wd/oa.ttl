#
# http://www.w3.org/ns/oa#bodyText

oa:bodyText a owl:DatatypeProperty , owl:FunctionalProperty ;
	rdfs:domain oa:Annotation ;
  rdfs:domain
		[ a owl:Restriction ;
			owl:onProperty oa:hasBody ;
			owl:maxCardinality "0"^^xsd:nonNegativeInteger ] ;
  rdfs:range xsd:string ;
	rdfs:label "bodyText"@en ;
	rdfs:comment """The object of the predicate is a plain text string to be used as
the content of the body of the Annotation. The value MUST be an xsd:string and
that data type MUST NOT be expressed in the serialization. Note that language
MUST NOT be associated with the value either as a language tag, as that is
only available for rdf:langString.
	"""@en ;
	rdfs:seeAlso <https://www.w3.org/TR/2016/WD-annotation-vocab-20160331/#bodytext> ;
	rdfs:isDefinedBy <http://www.w3.org/ns/oa#> .
#
# http://www.w3.org/ns/oa#end

oa:end a owl:DatatypeProperty ;
	rdfs:domain oa:Selector ;
	rdfs:range xsd:nonNegativeInteger ; # NOTE: this is xsd:integer in HTML
	rdfs:comment """The end position of the segment of text or bytes. The first character/byte in the full text/stream is position 0. The character/byte indicated at position oa:end is NOT included within the selected segment.

See oa:DataPositionSelector and oa:oa:TextPositionSelector."""@en ;
	rdfs:isDefinedBy <http://www.w3.org/ns/oa#> ;
	rdfs:seeAlso oa:DataPositionSelector, oa:TextPositionSelector, oa:start ;
	rdfs:seeAlso <https://www.w3.org/TR/2016/WD-annotation-vocab-20160331/#end> ;
	rdfs:label "end"@en .
#
# http://www.w3.org/ns/oa#exact

oa:exact a owl:DatatypeProperty ;
	#rdfs:domain oa:TextQuoteSelector ; ## TODO: Why is domain not explicit?
	rdfs:range xsd:string ;
	rdfs:comment """A copy of the text which is being selected, after normalization."""@en ;
	rdfs:seeAlso oa:TextQuoteSelector ;
	rdfs:isDefinedBy <http://www.w3.org/ns/oa#> ;
	rdfs:seeAlso <https://www.w3.org/TR/2016/WD-annotation-vocab-20160331/#exact> ;
	rdfs:label "exact"@en .
#
# http://www.w3.org/ns/oa#prefix

oa:prefix a owl:DatatypeProperty ;
	#rdfs:domain oa:TextQuoteSelector ; ## TODO: Why is domain not explicit?
	rdfs:seeAlso oa:TextQuoteSelector, oa:suffix ;
	rdfs:range xsd:string ;
	rdfs:comment """The object of the property is a snippet of content that occurs immediately before the content which is being selected by the Selector."""@en ;
	rdfs:isDefinedBy <http://www.w3.org/ns/oa#> ;
	rdfs:seeAlso <https://www.w3.org/TR/2016/WD-annotation-vocab-20160331/#prefix> ;
	rdfs:label "prefix"@en .
#
# http://www.w3.org/ns/oa#sourceDate

oa:sourceDate a owl:DatatypeProperty ;
	rdfs:domain oa:TimeState ;
	rdfs:domain
		[ a owl:Restriction ;
			owl:onProperty oa:sourceDateEnd ;
			owl:maxQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ;
	rdfs:domain
		[ a owl:Restriction ;
			owl:onProperty oa:sourceDateStart ;
			owl:maxQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ;
	rdfs:range xsd:dateTime ;
	rdfs:comment """The timestamp at which the Source resource should be interpreted as being applicable to the Annotation."""@en ;
	rdfs:isDefinedBy <http://www.w3.org/ns/oa#> ;
	rdfs:seeAlso <https://www.w3.org/TR/2016/WD-annotation-vocab-20160331/#sourcedate>;
	rdfs:label "sourceDate"@en .
#
# http://www.w3.org/ns/oa#sourceDateEnd

oa:sourceDateEnd a owl:DatatypeProperty , owl:FunctionalProperty ;
	rdfs:domain oa:TimeState ;
	rdfs:domain
		[
			a owl:Restriction ;
			owl:onProperty oa:sourceDateStart ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		];
	rdfs:range xsd:dateTime ;
	rdfs:comment """The end timestamp of the interval over which the Source resource should be interpreted as being applicable to the Annotation."""@en ;
	rdfs:isDefinedBy <http://www.w3.org/ns/oa#> ;
	rdfs:seeAlso oa:sourceDateStart;
	rdfs:seeAlso <https://www.w3.org/TR/2016/WD-annotation-vocab-20160331/#sourcedateend>;
	rdfs:label "sourceDateEnd"@en .
#
# http://www.w3.org/ns/oa#sourceDateStart

oa:sourceDateStart a owl:DatatypeProperty , owl:FunctionalProperty ;
	rdfs:domain oa:TimeState ;
	rdfs:domain
		[ a owl:Restriction ;
			owl:onProperty oa:sourceDateEnd ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		];
	rdfs:range xsd:dateTime ;
	rdfs:comment """The start timestamp of the interval over which the Source resource should be interpreted as being applicable to the Annotation."""@en ;
	rdfs:isDefinedBy <http://www.w3.org/ns/oa#> ;
	rdfs:seeAlso oa:sourceDateEnd;
	rdfs:seeAlso <https://www.w3.org/TR/2016/WD-annotation-vocab-20160331/#sourcedatestart>;
	rdfs:label "sourceDateStart"@en .
#
# http://www.w3.org/ns/oa#start

oa:start a owl:DatatypeProperty ;
	rdfs:domain oa:Selector ;
	rdfs:range xsd:nonNegativeInteger ; # NOTE: this is xsd:integer in HTML
	rdfs:comment """The start position in a 0-based index at which a range of content is selected from the data in the source resource."""@en ;
	rdfs:seeAlso oa:DataPositionSelector, oa:TextPositionSelector, oa:end ;
	rdfs:isDefinedBy <http://www.w3.org/ns/oa#> ;
	rdfs:seeAlso <https://www.w3.org/TR/2016/WD-annotation-vocab-20160331/#start> ;
	rdfs:label "start"@en .
#
# http://www.w3.org/ns/oa#styleClass

oa:styleClass a owl:DatatypeProperty ;
	rdfs:domain oa:SpecificResource ;
	rdfs:comment """The name of the class used in the CSS description referenced from the Annotation that should be applied to the Specific Resource."""@en ;
	rdfs:range xsd:string ;
	rdfs:seeAlso oa:CssStyle ;
	rdfs:isDefinedBy <http://www.w3.org/ns/oa#> ;
	rdfs:seeAlso <https://www.w3.org/TR/2016/WD-annotation-vocab-20160331/#styleclass> ;
	rdfs:label "styleClass"@en .
#
# http://www.w3.org/ns/oa#suffix

oa:suffix a owl:DatatypeProperty ;
	#rdfs:domain oa:TextQuoteSelector ; ## TODO: Why is domain not explicit?
	rdfs:comment """The snippet of text that occurs immediately after the text which is being selected."""@en ;
	rdfs:seeAlso oa:TextQuoteSelector, oa:prefix ;
	rdfs:isDefinedBy <http://www.w3.org/ns/oa#> ;
	rdfs:seeAlso oa:TextQuoteSelector ;
	rdfs:seeAlso <https://www.w3.org/TR/2016/WD-annotation-vocab-20160331/#suffix> ;
	rdfs:label "suffix"@en .
#
# http://www.w3.org/ns/oa#text

oa:text a owl:DatatypeProperty , owl:FunctionalProperty ;
	rdfs:domain oa:TextualBody ;
	rdfs:range xsd:string ;
	rdfs:comment """The content of a resource, given as text."""@en ;
	rdfs:isDefinedBy <http://www.w3.org/ns/oa#> ;
	rdfs:seeAlso <https://www.w3.org/TR/2016/WD-annotation-vocab-20160331/#text> ;
	rdfs:label "text"@en .
#
# http://www.w3.org/ns/oa#when

oa:when a owl:DatatypeProperty ;
	rdfs:domain oa:TimeState ;
	rdfs:range xsd:dateTimeStamp ;
	rdfs:isDefinedBy <http://www.w3.org/ns/oa#> , "true"^^xsd:boolean .
